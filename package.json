{
  "name": "react-2048",
  "version": "0.0.0",
  "description": "A clone of 2048 game with React",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config scripts/webpack.prod.js",
    "start": "webpack serve --config scripts/webpack.dev.js",
    "start:host": "webpack serve --config scripts/webpack.dev.js --host 0.0.0.0",
    "lint": "eslint . --ext .js,.ts,.tsx --max-warnings 0",
    "format": "prettier --write .",
    "tsc": "tsc -p tsconfig.json --noEmit",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "React",
    "2048",
    "JavaScript"
  ],
  "author": "Kai Wang",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.1.3",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run tsc"
    }
  }
}
